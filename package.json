{
  "name": "Miikun",
  "version": "1.1.0",
  "description": "Markdown Editor",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "build": "npm run build:js && npm run build:css",
    "build:js": "node ./build/vendorJS.js",
    "build:css": "node ./build/vendorCSS.js",
    "build:package": "electron-packager . Miikun --platform=darwin,win32 --arch=x64 --version=0.37.2",
    "build:win": "electron-packager . Miikun --platform=win32 --arch=x64 --version=0.37.2",
    "build:osx": "electron-packager . Miikun --platform=darwin --arch=x64 --version=0.37.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hiro0218/editor.git"
  },
  "keywords": [],
  "author": "hiro",
  "license": "ISC",
  "devDependencies": {
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.37.2"
  },
  "dependencies": {
    "clean-css": "^3.4.11",
    "codemirror-textlint": "^1.0.1",
    "markdown-it": "^6.0.1",
    "textlint-rule-incremental-headers": "^0.2.0",
    "textlint-rule-max-appearence-count-of-words": "^1.0.1",
    "textlint-rule-max-ten": "^2.0.1",
    "textlint-rule-no-double-negative-ja": "^1.0.3",
    "textlint-rule-no-doubled-conjunction": "^1.0.1",
    "textlint-rule-no-doubled-conjunctive-particle-ga": "^1.0.2",
    "textlint-rule-no-doubled-joshi": "^3.2.0",
    "textlint-rule-no-mix-dearu-desumasu": "^1.4.0",
    "uglify-js": "^2.6.2",
    "vue": "^1.0.20"
  }
}
