{
  "name": "miikun",
  "version": "3.2.1",
  "private": true,
  "description": "A Simple Markdown Editor",
  "homepage": "https://github.com/hiro0218/Miikun#readme",
  "bugs": {
    "url": "https://github.com/hiro0218/Miikun/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiro0218/Miikun.git"
  },
  "license": "MIT",
  "author": "hiro <hiro0218@gmail.com>",
  "main": "background.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:scss": "stylelint ./src",
    "format:scss": "stylelint --fix ./src",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "dependencies": {
    "debounce": "^1.2.0",
    "markdown-it": "^9.0.1",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-footnote": "^3.0.2",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.15.0",
    "vue": "^2.6.10",
    "vue-codemirror": "^4.0.6",
    "vue-electron": "^1.0.6",
    "vue-material": "^1.0.0-beta-11",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@hiro0218/eslint-config": "^1.0.4",
    "@hiro0218/prettier-config": "^1.0.1",
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.3",
    "babel-eslint": "^10.0.2",
    "electron": "^5.0.7",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.0.1",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "markdown-it-anchor": "^5.2.4",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-multimd-table": "^3.2.0-rc.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-property-sort-order-smacss": "^5.0.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-scss": "^3.9.2",
    "vue-cli-plugin-electron-builder": "^1.3.6",
    "vue-template-compiler": "^2.6.10"
